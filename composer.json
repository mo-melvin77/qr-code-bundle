{
    "name": "endroid/qrcode-bundle",
    "description": "Endroid QR Code Bundle",
    "keywords": ["endroid", "qr", "code", "symfony", "bundle", "php"],
    "homepage": "https://github.com/endroid/EndroidQrCodeBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van den Enden",
            "email": "info@endroid.nl",
            "homepage": "https://endroid.nl/"
        }
    ],
    "require": {
        "php": ">=7.1",
        "endroid/qrcode": "^3.1",
        "symfony/twig-bundle": "^3.0|^4.0",
        "twig/twig": "^1.34.2|^2.4.4",
        "symfony/http-foundation": "^3.2|^4.0",
        "symfony/dependency-injection": "^3.3|^4.0",
        "symfony/config": "^3.3|^4.0",
        "symfony/yaml": "^3.3|^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0",
        "symfony/framework-bundle": "^3.3|^4.0",
        "symfony/phpunit-bridge": "^3.3|^4.0",
        "symfony/routing": "^3.3|^4.0",
        "symfony/browser-kit": "^3.3|^3.4"
    },
    "suggest": {
        "symfony/routing": "To enable controllers that generate QR Codes."
    },
    "autoload": {
        "psr-4": {
            "Endroid\\QrCodeBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Endroid\\QrCodeBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    }
}
